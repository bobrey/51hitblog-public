[core]
	excludesfile = /Users/hikari-user/.gitignore
	editor = vim
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[user]
	name = kaku
	email = xxx@xxx.com
[push]
	default = simple
[alias]
	ci = commit -a -v
	st = status
	rb = pull --rebase
[color]
	ui = true
